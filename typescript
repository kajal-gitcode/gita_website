server {
    listen 80;
    server_name bridgelab.cse.iitk.ac.in;
    return 301 https://$host$request_uri;
}

server {
    listen 443 ssl;
    server_name bridgelab.cse.iitk.ac.in;

    ssl_certificate /etc/letsencrypt/live/bridgelab.cse.iitk.ac.in/fullchain.pem;
    ssl_certificate_key /etc/letsencrypt/live/bridgelab.cse.iitk.ac.in/privkey.pem;
    include /etc/letsencrypt/options-ssl-nginx.conf;
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem;

    # --- Backend API ---
    # Normalize both /api/... and /api/api/... to single /api/ path
    location ~ ^/api(/api)?/ {
        rewrite ^/api/api/(.*)$ /api/$1 break;
        proxy_pass http://127.0.0.1:8081;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection 'upgrade';
        proxy_set_header Host $host;
        proxy_cache_bypass $http_upgrade;
    }

    # --- Frontend React build ---
    root /home/ubuntu/carbon-data-analysis/frontend/build;
    index index.html;

    location / {
        try_files $uri /index.html;
    }

    
    # --- Gita Supersite ---
    location ^~ /gitasupersite/ {
        alias /var/www/gitasupersite/;
        index index.html;
        try_files $uri $uri/ /gitasupersite/index.html;
    }

    # Serve static, media, and asset files correctly
    location ~* ^/gitasupersite/(static|assets|media)/ {
    	alias /var/www/gitasupersite/$1/;
    	access_log off;
    	expires 30d;
    	add_header Cache-Control "public, no-transform";
    }
    location ^~ /gitasupersite/static/ {
	alias /var/www/gitasupersite/static/;
    	access_log off;
    	expires 1y;
    }
    

    location ~* \.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot|otf|map)$ {
        try_files $uri =404;
        expires 1y;
        access_log off;
    }

    error_log /var/log/nginx/bridgelab_error.log;
    access_log /var/log/nginx/bridgelab_access.log;

}
